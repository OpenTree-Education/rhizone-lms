    "@mdx-js/react": "^1.6.22",
    "@mui/material": "^5.2.1",
    "@reach/router": "^1.3.4",
    "@types/react-helmet": "^6.1.4",
    "babel-eslint": "^10.1.0",
    "eslint-config-react-app": "^6.0.0",
    "gatsby": "^4.2.0",
    "gatsby-image": "^3.11.0",
